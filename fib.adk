#include anr

funct Fib(n) {
    if number(n) < 0 {
        output('invalid input')
    }
    if number(n) == 1 {
        return 0
    }
    if number(n) == 2 {
        return 1
    }
    if number(n) > 2 {
        n=number(n)
        x=Fib(n-2)
        a=Fib(n-1) 
        return a+x
    }
}

output(Fib(input("Enter a number to do Fib on: ")))
