#include anr

funct fibo(n) {
    output("welcome")
    if number(n) < 0 {
        output('invalid input')
    }
    if number(n) == 1 {
        output(0)
    }
    if number(n) == 2 {
        output(1)
    }
    if number(n) > 2 {
        n=number(n)
        x=fibo(n-2)
        a=fibo(n-1) 
        output(a+x)
    }
}

fibo(30)
